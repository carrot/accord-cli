    "hogan.js": "*",
    "handlebars": "*",
    "hamljs": "*",
    "stylus": "*",
    "node-sass": "*",
    "less": "*",
    "myth": "*",
    "dogescript": "*",
    "coco": "*",
    "LiveScript": "*",
    "uglifyjs": "*",
    "clean-css": "*",
    "html-minifier": "*",
